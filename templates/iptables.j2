
# InfluxDB
-A INPUT -p tcp -m multiport --dports {{ influxdb_admin_port }},{{ influxdb_api_port }},{{ influxdb_proto_port }} -j ACCEPT
-A INPUT -p udp -m multiport --dports {{ influxdb_admin_port }},{{ influxdb_api_port }},{{ influxdb_proto_port }} -j ACCEPT

# InfluxDB Graphite Port
{% if influxdb_graphite_plugin_enabled == "true" %}
-A INPUT -p tcp --dport {{ influxdb_graphite_plugin_port }} -j ACCEPT
{% endif %}

